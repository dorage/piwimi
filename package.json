{
    "name": "psychotest",
    "private": true,
    "scripts": {
        "test": "cross-env NODE_ENV=development babel-node test/index.js",
        "build": "webpack --mode production",
        "watch": "webpack --watch --mode development",
        "dev-server": "nodemon --delay 300ms --exec cross-env NODE_ENV=development babel-node src/index.js",
        "start": "npm run dev-server",
        "production": "npm run build && cross-env NODE_ENV=production babel-node src/index.js",
        "forever": "sudo forever start -c \"npm run production\" ./",
        "nginx": "sudo systemctl restart nginx",
        "on8080": "sudo \\cp -f /root/8080/.env.production /root/you_i_psy/.env.production && sudo \\cp -f /root/8080/nginx.conf /etc/nginx/nginx.conf && sudo npm run forever",
        "on8081": "sudo \\cp -f /root/8081/.env.production /root/you_i_psy/.env.production && sudo \\cp -f /root/8081/nginx.conf /etc/nginx/nginx.conf && sudo npm run forever"
    },
    "dependencies": {
        "connect-pg-simple": "^7.0.0",
        "cross-env": "^7.0.2",
        "express": "^4.17.1",
        "express-session": "^1.17.1",
        "googleapis": "^88.2.0",
        "helmet": "^3.22.0",
        "mongoose": "^5.12.9",
        "morgan": "^1.10.0",
        "node-fetch": "^2.6.1",
        "pg": "^8.7.1",
        "pug": "^3.0.2",
        "serve-favicon": "^2.5.0",
        "style-loader": "^2.0.0"
    },
    "devDependencies": {
        "@sentry/browser": "^6.8.0",
        "@sentry/node": "^5.30.0",
        "@sentry/tracing": "^6.13.3",
        "@babel/cli": "^7.12.1",
        "@babel/core": "^7.10.1",
        "@babel/node": "^7.10.1",
        "@babel/plugin-transform-runtime": "^7.10.1",
        "@babel/polyfill": "^7.10.1",
        "@babel/preset-env": "^7.10.1",
        "@babel/runtime": "^7.10.1",
        "css-loader": "^5.2.6",
        "dotenv": "^8.2.0",
        "eslint": "^7.32.0",
        "eslint-config-node": "^2.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.4.1",
        "file-loader": "^6.2.0",
        "kill-port": "^1.6.1",
        "mini-css-extract-plugin": "^1.6.0",
        "nodemon": "^2.0.7",
        "prettier": "^2.3.2",
        "sass": "^1.34.1",
        "sass-loader": "^11.1.1",
        "webpack": "^5.38.1",
        "webpack-cli": "^4.7.0"
    }
}
