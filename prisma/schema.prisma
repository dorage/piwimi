// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

model PsyReview {
  id        String   @id @default(dbgenerated()) @map("_id") @db.ObjectId
  review    String
  createdAt DateTime @default(now())
}

model PsyQuery {
  id     String    @id @default(dbgenerated()) @map("_id") @db.ObjectId
  psy    PsyYesNo? @relation(fields: [psyId], references: [id])
  psyId  String?   @db.ObjectId
  query  String
  imgUrl String
}

model PsyYesNo {
  id      String     @id @default(dbgenerated()) @map("_id") @db.ObjectId
  quries  PsyQuery[]
  weights Int[]
}

model PsyType {
  id   String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  type String
}
